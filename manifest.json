{
  "manifest_version": 3,
  "name": "linkedin scripting",
  "version": "1.0",
  "description": "Scrapes LinkedIn profiles and posts to API",
  "permissions": ["tabs" , "scripting"],
  "host_permissions": [
  "https://www.linkedin.com/*",
  "http://localhost:3000/*"
],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },

"icons" : {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png"
    }
}
